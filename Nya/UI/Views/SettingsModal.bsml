<bg id="root" xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd'>
	<modal id='modal' show-event='open-modal' hide-event='close-modal' clickerino-offerino-closerino='true' size-delta-x='60' size-delta-y='30' move-to-center='true' horizontal-fit='PreferredSize' vertical-fit='PreferredSize'>
		<button text='x' click-event='close-modal' active='true' anchor-pos-x='54'/>
		<vertical horizontal-fit='PreferredSize' vertical-fit='PreferredSize' preferred-width='50' preferred-height='30' spacing='-5'>
			<button id='nyaDownloadButton' text='Download Nya' on-click='nya-download-click' active='true' hover-hint='Download the current nya to UserData\Nya'/>
			<button id='nyaCopyButton' text='Copy Nya' on-click='nya-copy-click' active='true' hover-hint='Copy the current nya to clipboard'/>
			<checkbox-setting id='nsfwCheck' text='NSFW Nya' value='nya-nsfw-check' apply-on-change='true' bind-value='true'></checkbox-setting>
		</vertical>
	</modal>
</bg>